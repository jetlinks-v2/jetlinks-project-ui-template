{
  "name": "jetlinks-ui-template",
  "private": true,
  "version": "2.1.1",
  "scripts": {
    "dev": "vite",
    "dev:force": "vite --force",
    "build": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build",
    "build:server": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build --mode server -- --modules=no-modules",
    "preview": "vite preview",
    "clean": "pnpm rimraf node_modules && pnpm rimraf dist && pnpm rimraf .turbo",
    "modules:init": "git submodule init && git submodule update && git submodule foreach git checkout master && git submodule foreach git pull origin master",
    "modules:update": "git submodule update",
    "build:modules": "pnpm build --mode server --",
    "build:module": "node scripts/build-module.js",
    "create:module": "node ./scripts/template.js create"
  },
  "dependencies": {
